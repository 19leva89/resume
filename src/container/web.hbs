{{!
Це код container файлу для index сторінки,
в якому міститься код, котрий буде віддаватися по шляху (PATH) - "/"
}}
<div>

  <div>
    <h1>Languages</h1>
  </div>

  {{#with web}}

    {{#each languages}}
      <ul>
        <hr />
        <li>Name: {{name}}</li>
        <li>Version: {{version}}</li>
        <li>Description: {{description}}</li>

        {{#each elements}}
          <li>Elements:</li>
          <ul>
            <li>{{name}}</li>
            <li>{{description}}</li>

            {{#each attributes}}
              <li>Attributes:</li>
              <ul>
                <li>{{name}}</li>
                <li>{{description}}</li>
              </ul>
            {{/each}}

          </ul>
        {{/each}}

        {{#each properties}}
          <li>Properties:</li>
          <ul>
            <li>{{name}}</li>
            <li>{{description}}</li>

            {{#each values}}
              <li>Values:</li>
              <ul>
                <li>{{value}}</li>
                <li>{{description}}</li>
              </ul>
            {{/each}}

          </ul>
        {{/each}}

        {{#each functions}}
          <li>Functions:</li>
          <ul>
            <li>{{name}}</li>
            <li>{{description}}</li>

            {{#each parameters}}
              <li>Parameters:</li>
              <ul>
                <li>{{name}}</li>
                <li>{{type}}</li>
                <li>{{description}}</li>
              </ul>
            {{/each}}

          </ul>
        {{/each}}

        <br />
      </ul>
    {{/each}}

  {{/with}}
</div>
