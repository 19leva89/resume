<main>

	<div class="row">

		{{#with navigation}}
		<nav class="row">

			{{#each links}}
			<a href="{{href}}" class="col-1 btn btn-link">{{text}}</a>
			<div class="col-1"></div>
			{{/each}}

			{{#with button}}
			<a href="{{href}}" class="col-2 btn btn-primary">{{text}}</a>
			{{/with}}

		</nav>
		{{/with}}

		<hr>

		{{#with header}}
		<section class="col-12">
			<h1>{{title}}</h1>
			<p>{{description}}</p>

			{{#with button}}
			<a href="{{link}}" class="btn btn-primary">{{text}}</a>
			{{/with}}

		</section>
		{{/with}}

		<hr>

		{{#with city}}
		<section class="col-12">

			<h2>{{title}}</h2>

			<br>


			<div class="row gy-4 gx-2">
				{{#each cards}}
				<div class="col-5">
					<div class="card">

						{{#with picture}}
						<img class="card-img-top" src="{{src}}" alt="{{alt}}">
						{{/with}}

						<div class="card-body">
							<h5 class="card-title">{{title}}</h5>
							<p>{{description}}</p>

							{{#with button}}
							<a href="{{href}}" class="btn btn-primary">{{text}}</a>
							{{/with}}

						</div>
					</div>
				</div>
				{{/each}}
			</div>

		</section>
		{{/with}}

		<hr>

		{{#with winner}}
		<section class="col-12">

			<h2>{{title}}</h2>
			<br>

			{{#with table}}
			<table class="table">


				<thead>
					<tr>
						{{#each head}}
						<th>{{this}}</th>
						{{/each}}
					</tr>
				</thead>

				<tbody>

					{{#each user}}
					<tr>
						<th>{{place}}</th>
						<td>{{name}}</td>
						<td>{{surname}}</td>
						<td>{{nickname}}</td>
					</tr>
					{{/each}}

				</tbody>
			</table>
			{{/with}}

		</section>
		{{/with}}

		<hr>

		{{#with gallery}}
		<h2>{{title}}</h2>

		<section class="row gx-2">

			{{#each cards}}
			<div class="col-4">
				<div class="card">
					<div class="card-header">
						<div>{{name}}</div>
					</div>
					<ul class="list-group list-group-flush">
						<li class="list-group-item">{{items}}</li>
					</ul>
				</div>
			</div>
			{{/each}}

		</section>
		{{/with}}

		<br>
		<hr>
		{{#with relaxion}}
		<div class="display-3">{{title}}</div>

		<section class="col-12">

			<div class="row gx-2 gy-4">

				{{#each cards}}
				<div class="col-5">
					<div class="card text-bg-secondary">
						<div class="card-header">{{header}}</div>
						<div class="card-body">
							<h6 class="card-title">{{title}}</h6>
							<p>{{description}}</p>
						</div>
					</div>
				</div>
				{{/each}}

			</div>
		</section>
		{{/with}}

		<br>
		<hr>

		{{#with rest}}
		<section class="col-12">

			<div class="row g-2">

				{{#each cards}}
				<div class="col-6">
					<div class="card">
						<div class="card-header text-center">{{header}}</div>
						<div class="card-body">
							<h6 class="card-title text-center">{{title}}</h6>
							<p class="text-center">{{description}}</p>
							<button class="btn btn-primary">{{button}}</button>
						</div>
						<div class="card-footer text-center">{{footer}}</div>
					</div>
				</div>
				{{/each}}

			</div>
		</section>
		{{/with}}

		<br>
		<hr>

		{{#with proposition}}
		<div class="display-2">{{title}}</div>

		<section class="col-12">
			<div class="row gx-2 gy-4">

				{{#each card}}
				<div class="col-5">
					<div class="card text-bg-warning">
						<div class="card-header">{{header}}</div>
						<div class="card-body">
							<h6 class="card-title">{{title}}</h6>
							<p>{{description}}</p>
						</div>
					</div>
				</div>
				{{/each}}

			</div>
		</section>
		{{/with}}

		<br>
		<hr>

		{{#with footer}}
		<h5>{{title}}</h5>

		<div class="col-3">
			<div class="row gy-2">

				{{#each navigation}}
				<div>
					<a href="{{href}}" class=" btn btn-link">{{text}}</a>
				</div>
				{{/each}}

			</div>
		</div>

		<div class="col-3">
			<div class="row gy-2">

				{{#each socialMedia}}
				<div>
					<a href="{{link}}" class=" btn btn-outline-dark">{{name}}</a>
				</div>
				{{/each}}

			</div>
		</div>

		<div class="col-3">
			<div class="row gy-2">

				{{#each partners}}
				<div>
					<a href="{{link}}" class=" btn btn-outline-info">{{name}}</a>
				</div>
				{{/each}}

			</div>
		</div>

		{{/with}}

	</div>
</main>