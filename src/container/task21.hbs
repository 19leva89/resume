<main>

	{{#with heading}}
	<div class="card">
		<div class="card-body">
			{{#with main}}
			<div class="card">
				<div class="card-body">
					<h1>{{title}}</h1>
					<p>{{text}}</p>

					{{#with button_text}}
					<div>
						<button class="btn btn-primary">{{solid}}</button>
					</div>
					<br>
					<div>
						<button class="btn btn-outline-primary">{{outline}}</button>
					</div>
					{{/with}}

				</div>
			</div>
			{{/with}}

			<br>

			{{#with second}}
			<div class="card">
				<div class="card-body">
					<h2>{{title}}</h2>
					<p>{{text}}</p>

					{{#with button_text}}
					<div>
						<button class="btn btn-secondary">{{solid}}</button>
					</div>
					<br>
					<div>
						<button class="btn btn-outline-secondary">{{outline}}</button>
					</div>
					{{/with}}

				</div>
			</div>
			{{/with}}

			<br>

			{{#with subblock}}
			<div class="card">
				<div class="card-body">
					<h3>{{title}}</h3>
					<p>{{text}}</p>

					{{#with button_text}}
					<div>
						<button class="btn btn-dark">{{solid}}</button>
					</div>
					<br>
					<div>
						<button class="btn btn-outline-dark">{{outline}}</button>
					</div>
					{{/with}}

				</div>
			</div>
			{{/with}}

		</div>
	</div>
	{{/with}}

	<br>

	{{#with nested_display}}
	<div class="card">
		<div class="card-body">
			<div class="display-1">{{title}}</div>

			<br>

			{{#with children_display}}
			<div class="card">
				<div class="card-body">
					<div class="display-4">{{title}}</div>
					<p>{{text}}</p>

					{{#with button}}
					<div>
						<button class="btn btn-outline-warning">{{text}}</button>
					</div>
					{{/with}}

				</div>
			</div>
			{{/with}}

			<br>

			<h6>{{header_text}}</h6>

			<br>

			{{#with button}}
			<div>
				<button class="btn btn-info text-light">{{text}}</button>
			</div>
			{{/with}}

		</div>
	</div>
	{{/with}}

	<br>

	{{#with paragraph}}
	<div class="card">
		<div class="card-body">
			<p class="text-uppercase">{{title}}</p>
			<p class="text-lowercase">{{text}}</p>

			{{#with button}}
			<div>
				<a href="{{url}}" class="btn btn-link">{{text}}</a>
			</div>
			{{/with}}

			<br>

			<div class="card">
				<div class="card-body">

					{{#each list_name}}
					<ul>
						<li>
							<a href="{{url}}" class="btn btn-link">{{text}}</a>
						</li>
					</ul>
					{{/each}}

				</div>
			</div>

		</div>
	</div>
	{{/with}}

</main>